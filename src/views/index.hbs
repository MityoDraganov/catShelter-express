
    <main>
        <section class="cats">
            <ul>
                {{#each cats}}
                <li>
                    
                    <img src="https://ichef.bbci.co.uk/news/976/cpsprodpb/12A9B/production/_111434467_gettyimages-1143489763.jpg" alt="Black Cat">
                    <h3>{{name}}</h3>
                    <p><span>Breed: </span>{{breed}}</p>
                    <p><span>Description: </span>{{description}}</p>
                    <ul class="buttons">
                        <li class="btn edit"><a href="/cats/edit/{{id}}">Change Info</a></li>
                        <li class="btn delete"><a href="/cats/delete/{{id}}">New Home</a></li>
                    </ul>
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
